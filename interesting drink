#include <bits/stdc++.h>
using namespace std;

int main() {
 int n,b,g,c;
 cin >> n;
 vector<int>a(n);
 for(int i=0;i<n;i++)
   cin >>a[i];
   
 sort(a.begin(),a.end());
 cin >> b;
 for(int i=0;i<b;i++){
  cin >>g;
     c= upper_bound(a.begin(),a.end(),g)-a.begin();
    cout<<c<<endl;
 }
 return 0;
}
