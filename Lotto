#include <iostream>
#include <bits/stdc++.h>
using namespace std;

string x;
int main()
{
     ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

   int n;
   cin>>n;

   while(n)
   {
       int a[13];


       for(int i=0;i<n;i++)
       {
           cin>>a[i];

       }

       for(int i=0;i<n;i++)
       {
           for(int j=i+1;j<n;j++)
       {
           for(int k=j+1;k<n;k++)
       {
           for(int l=k+1;l<n;l++)
       {
           for(int m=l+1;m<n;m++)
       {
           for(int q=m+1;q<n;q++)
       {
          
            if(i==j||j==k||k==l||l==m||m==q)
                continue;
            else
                cout<<a[i]<<" "<<a[j]<<" "<<a[k]<<" "<<a[l]<<" "<<a[m]<<" "<<a[q]<<endl;
       }}}}}}

       cin>>n;
       if(n!=0)
        cout<<endl;

   }
    return 0;
}
